#include <iostream>

using namespace std;

int main() {
    int number;

    // Input the integer
    cout << "Enter an integer: ";
    cin >> number;

    // Check if the number is divisible by 2
    if (number % 2 == 0) {
        cout << number << " is even." << endl;
    } else {
        cout << number << " is odd." << endl;
    }

    return 0;
}
